import java.util.*;
public class javabasics {
    public static void main(String args[]) {
        int a = 25;
        long b = a;
        System.out.println(b);
        /* conversion happen when :
        a) type compatible
        b) destination type > source ttpe
        
        byte > short > int > float > long > double
        */
        
        
        
    }
}
